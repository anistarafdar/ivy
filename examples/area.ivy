(program 

    (def circle_area (lambda (:radius)
        (* 3.14 (* radius radius))))

    (def rectangle_area (lambda (:length width)
        (* length width)))
    
    (def r 6)
    (def l 5)
    (def w 10)

    (println "circle_area(" r ")=" (circle_area r))
    (println "rectangle_area(" l "," w ")=" (rectangle_area l w))
)